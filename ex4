using System;

class Program
{
    static void Main()
    {
        int numero = 5;
        MostraTabuada(numero);
    }

    static void MostraTabuada(int numero)
    {
        Console.WriteLine($"Tabuada do {numero}:");

        // Loop para mostrar a tabuada do n√∫mero
        for (int i = 1; i <= 10; i++)
        {
            Console.WriteLine($"{numero} x {i} = {numero * i}");
        }
    }
}